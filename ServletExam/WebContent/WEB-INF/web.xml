<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>ServletExam</display-name>
  
  
  <filter>
  	<filter-name>CustomCharacterEncoding</filter-name>
  	<filter-class>kr.or.ddit.comm.filter.CustomCharacterEncoding</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>CustomCharacterEncoding</filter-name>
  	<url-pattern>*.do</url-pattern>
  </filter-mapping>

  
  <filter>
  	<filter-name>LoginCheckFilter</filter-name>
  	<filter-class>kr.or.ddit.comm.filter.LoginCheckFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>LoginCheckFilter</filter-name>
  	<url-pattern>*.do</url-pattern>
  </filter-mapping>
  
  
  
</web-app>